{
  "name": "viterjs-template",
  "version": "1.1.7",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 8080",
    "start": "vite --port 8080",
    "host": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@mui/x-date-pickers": "^5.0.7",
    "add": "^2.0.6",
    "apisauce": "^2.1.6",
    "date-fns": "^2.29.3",
    "formik": "^2.2.9",
    "mobx": "^6.6.2",
    "mobx-react-lite": "^3.4.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "yarn": "^1.22.19",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-istanbul": "0.24.3",
    "eslint": "^8.25.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "vite": "^3.2.0",
    "vitest": "^0.24.3"
  }
}
